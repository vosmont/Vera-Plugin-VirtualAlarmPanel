<?xml version="1.0" encoding="utf-8"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>0</minor>
	</specVersion>
	<serviceStateTable>
		<!-- Panel -->
		<stateVariable>
			<name>Status</name>
			<sendEventsAttribute>yes</sendEventsAttribute>
			<dataType>ui1</dataType>
			<defaultValue>0</defaultValue>
			<shortCode>status</shortCode>
		</stateVariable>
		<stateVariable>
			<name>AlarmPanel</name>
			<sendEventsAttribute>yes</sendEventsAttribute>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
			<shortCode>alarmpanel</shortCode>
		</stateVariable>
		<stateVariable>
			<name>Alarms</name>
			<sendEventsAttribute>yes</sendEventsAttribute>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
			<shortCode>alarms</shortCode>
		</stateVariable>
		<stateVariable>
			<name>LastActiveAlarmName</name>
			<sendEventsAttribute>yes</sendEventsAttribute>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
			<shortCode>lastactivealarmname</shortCode>
		</stateVariable>
		<stateVariable>
			<name>LastActiveAlarmId</name>
			<sendEventsAttribute>yes</sendEventsAttribute>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
			<shortCode>lastactivealarmid</shortCode>
		</stateVariable>
		<!-- Options -->
		<stateVariable>
			<name>Options</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!-- Arguments -->
		<stateVariable>
			<name>A_ARG_TYPE_Boolean</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>boolean</dataType>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_AlarmId</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>ui1</dataType>
			<allowedValueRange>
				<minimum>1</minimum>
				<maximum>20</maximum>
			</allowedValueRange>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_AlarmPos</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>ui1</dataType>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_AlarmName</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_AlarmMode</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
			<defaultValue>AUTO</defaultValue>
			<allowedValueList>
				<allowedValue>AUTO</allowedValue>
				<allowedValue>MANUAL</allowedValue>
			</allowedValueList>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_AlarmType</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
			<allowedValueList>
				<allowedValue>TEMPERATURE</allowedValue>
				<allowedValue>OPENING</allowedValue>
				<allowedValue>INFORMATION</allowedValue>
				<allowedValue>MINOR</allowedValue>
				<allowedValue>MAJOR</allowedValue>
				<allowedValue>CRITICAL</allowedValue>
				<allowedValue>UNKNOWN</allowedValue>
			</allowedValueList>
		</stateVariable>
		<!-- Trick of the dummy argument for computed return -->
		<stateVariable>
			<name>LastResult</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
			<shortCode>lastresult</shortCode>
		</stateVariable>
		<stateVariable>
			<name>LastResult2</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
			<shortCode>lastresult2</shortCode>
		</stateVariable>
	</serviceStateTable>
	<actionList>
		<!-- Panel status -->
		<!--
		<action>
			<name>SetStatus</name>
			<argumentList>
				<argument>
					<name>newStatus</name>
					<direction>in</direction>
					<relatedStateVariable>Status</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		-->
		<action>
			<name>GetStatus</name>
			<argumentList>
				<argument>
					<name>retStatus</name>
					<direction>out</direction>
					<retval/>
					<relatedStateVariable>Status</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<!-- Alarm status -->
		<action>
			<name>SetAlarmStatus</name>
			<argumentList>
				<argument>
					<name>newStatus</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Boolean</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmMode</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Mode</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>GetAlarmStatus</name>
			<argumentList>
				<argument>
					<name>retStatus</name>
					<direction>out</direction>
					<retval/>
					<!--
					<relatedStateVariable>A_ARG_TYPE_Boolean</relatedStateVariable>
					-->
					<relatedStateVariable>LastResult</relatedStateVariable>
				</argument>
				<argument>
					<name>retLastUpdate</name>
					<direction>out</direction>
					<retval/>
					<relatedStateVariable>LastResult2</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<!-- Alarm acknowledge -->
		<action>
			<name>SetAlarmAcknowledge</name>
			<argumentList>
				<argument>
					<name>newAcknowledge</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Boolean</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>GetAlarmAcknowledge</name>
			<argumentList>
				<argument>
					<name>retAcknowledge</name>
					<direction>out</direction>
					<retval/>
					<!--
					<relatedStateVariable>A_ARG_TYPE_Boolean</relatedStateVariable>
					-->
					<relatedStateVariable>LastResult</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<!-- Alarm -->
		<action>
			<name>AddAlarm</name>
			<argumentList>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>RemoveAlarm</name>
			<argumentList>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetAlarmName</name>
			<argumentList>
				<argument>
					<name>newAlarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>GetAlarmName</name>
			<argumentList>
				<argument>
					<name>retAlarmName</name>
					<direction>out</direction>
					<!--
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
					-->
					<relatedStateVariable>LastResult</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>GetAlarmId</name>
			<argumentList>
				<argument>
					<name>retAlarmId</name>
					<direction>out</direction>
					<!--
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
					-->
					<relatedStateVariable>LastResult</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetAlarmPos</name>
			<argumentList>
				<argument>
					<name>newAlarmPos</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmPos</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<!-- Alarm type -->
		<action>
			<name>SetAlarmType</name>
			<argumentList>
				<argument>
					<name>newAlarmType</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmType</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>GetAlarmType</name>
			<argumentList>
				<argument>
					<name>retAlarmType</name>
					<direction>out</direction>
					<!--
					<relatedStateVariable>A_ARG_TYPE_AlarmType</relatedStateVariable>
					-->
					<relatedStateVariable>LastResult</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmId</relatedStateVariable>
				</argument>
				<argument>
					<name>alarmName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_AlarmName</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<!-- Options -->
		<action>
			<name>SetOptions</name>
			<argumentList>
				<argument>
					<name>newOptions</name>
					<direction>in</direction>
					<relatedStateVariable>Options</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
	</actionList>
</scpd>
